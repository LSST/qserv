echo '=== destroyMeta ==='
./admin/bin/metaTool.sh destroyMeta

echo '=== installMeta ==='
./admin/bin/metaTool.sh installMeta

echo '=== printMeta ==='
./admin/bin/metaTool.sh printMeta

echo '=== registerDb (missing arg) ==='
./admin/bin/metaTool.sh registerDb

echo '=== registerDb (db not in qms) ==='
./admin/bin/metaTool.sh registerDb xyzdd

echo '=== registerDb (all good) ==='
./admin/bin/metaTool.sh registerDb Summer2012
