echo '=== destroyMeta ==='
./admin/examples/run_qmwTool destroyMeta

echo '=== installMeta ==='
./admin/examples/run_qmwTool installMeta

echo '=== printMeta ==='
./admin/examples/run_qmwTool printMeta

echo '=== registerDb (missing arg) ==='
./admin/examples/run_qmwTool registerDb

echo '=== registerDb (db not in qms) ==='
./admin/examples/run_qmwTool registerDb xyzdd

echo '=== registerDb (all good) ==='
./admin/examples/run_qmwTool registerDb Summer2012
